<h1><%= @dog.name %></h1>

<h2>
   Breed: <%= @dog.breed%>
</h2>
<h2>
   Age: <%= @dog.age%>
</h2>

<h3>
    Owners:
</h3>
<h4>
    <% @dog.employees.each do |employee| %>
        <li>
            <%= link_to employee.full_name, 
            employee_path(employee) %>
        </li>
    <% end %>
</h4>